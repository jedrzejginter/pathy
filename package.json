  "keywords": [
    "path",
    "validation",
    "routing"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": "git@github.com:jedrzejginter/pathy.git",
  "bugs": {
    "url": "https://github.com/jedrzejginter/pathy/issues"
  },
  "author": {
    "name": "Jedrzej Ginter",
    "email": "dzeejj@gmail.com",
    "url": "https://github.com/jedrzejginter"
  },
  "license": "MIT",
  "engines": {
    "node": ">=13"
  },
  "files": [
    "lib"
  ],
  "scripts": {
